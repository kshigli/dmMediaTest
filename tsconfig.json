{
  "compilerOptions": {
      "skipLibCheck": true,
      "outDir": "dist",
      "sourceMap": false,
      "target": "ES2021",
      "module": "commonjs",
      "removeComments": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "types": ["node", "webdriverio/async", "@wdio/cucumber-framework"]
  }
}